{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "keyboard-auto-typing-app",
  "version": "0.1.0",
  "identifier": "com.keyboard-auto-typing-app.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:4440",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "keyboard-auto-typing-app",
        "minWidth": 1024,
        "minHeight": 600,
        "width": 1024,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/keyboard-psd.jpg",
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
